{
  "manifest_version": 2,

  "name": "fantasykeeper",
  "description": "Google Chrome Extension for ESPN Fantasy Baseball to display contract information, minor leaguer rosters, and advanced stats",
  "version": "0.01",

  "browser_action": {
    "default_icon": "baseballicon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ],
  "content_scripts": [ {
    "js": [ "js/contentscript.js" ],
    "matches": [ "*://games.espn.go.com/flb/clubhouse*", "*://games.espn.go.com/flb/freeagency*", "*://games.espn.go.com/flb/trade*", "http://games.espn.com/flb/clubhouse*", "http://games.espn.com/flb/freeagency*", "http://games.espn.com/flb/trade*", "http://games.espn.com/flb/watchlist*", "http://games.espn.com/flb/playerrater*", "http://games.espn.com/flb/leaders*", "http://games.espn.com/flb/*", "*://*.baseball.cbssports.com/teams*", "*://*.baseball.cbssports.com/stats/stats-main*", "*://*.baseball.cbssports.com/transactions/trade*", "*://baseball.fantasysports.yahoo.com/*" ]
  } ]
}